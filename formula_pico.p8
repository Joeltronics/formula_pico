#include utils.lua
#include minimap.lua
#include sound.lua
#include drawing.lua
#include title_screen.lua
#include game_logic.lua

function _init()
	init_title_screen()
end

function _update60()
	if road then
		game_tick()
		update_sound()
	else
		update_title_screen()
	end
end

function _draw()
	if road then
		draw_bg()
		draw_road()
		draw_minimap()
		draw_hud()
		if debug then
			draw_debug_overlay()
		elseif print_cpu then
			draw_cpu_only_overlay()
		end
	else
		draw_title_screen()
	end
end

